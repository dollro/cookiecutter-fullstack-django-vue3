    "simplelightbox": "^2.10.4",
    "vite-plugin-css-injected-by-js": "^3.3.0",
    "vue": "^3.3.4",
    "vue-i18n": "^10.0.7",
    "vue-plugin-django-utils": "^0.1.5"
